						#echo "Jenkinsfile Test3"
					'''
				}
			}


			stage('Test') {
				steps {
					sh '''
						#chmod +x DOTT/cidr_convert_api/go/convert_test.go
						#./DOTT/cidr_convert_api/go/convert_test.go
						echo "Step Two"
					'''
				}
			} 

			stage('Deploy') {
				steps {
					sh '''
						echo "Step Three"
					'''
				}
			}
		}
}
